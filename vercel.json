{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/account-general.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/top-languages.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/contributions-graph.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/minimal.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/debug.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/simple-svg.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/test.py"
    },
    {
      "src": "/api/minimal",
      "dest": "/api/minimal.py"
    },
    {
      "src": "/api/debug",
      "dest": "/api/debug.py"
    },
    {
      "src": "/api/simple-svg",
      "dest": "/api/simple-svg.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/contributions-graph",
      "dest": "/api/contributions-graph.py"
    },
    {
      "src": "/api/top-languages", 
      "dest": "/api/top-languages.py"
    },
    {
      "src": "/api/account-general",
      "dest": "/api/account-general.py"
    },
    {
      "src": "/",
      "dest": "/frontend/dist/index.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}
